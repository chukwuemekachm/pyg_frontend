{
  "name": "pyg_frontend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:dev": "rm -rf public && webpack --env.mode development",
    "build:prod": "rm -rf public && webpack --env.mode production",
    "build": "yarn build:prod",
    "prestart": "yarn build",
    "start": "node server.js",
    "dev": "yarn build:dev && rm -rf public && webpack-dev-server --env.mode development --open --hot",
    "test": "jest",
    "lint:fix": "rm -rf public && eslint '*/**/*.{js,jsx}' --quiet --fix",
    "lint": "rm -rf public && eslint '*/**/*.{js,jsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn lint:fix",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "connected-react-router": "^6.8.0",
    "express": "^4.17.3",
    "helmet": "^3.22.0",
    "history": "^4.10.1",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-datepicker": "^2.14.1",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "style-loader": "^1.1.3",
    "styled-components": "^5.0.1",
    "validatorjs": "^3.18.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-webpack-alias-7": "^0.1.1",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
